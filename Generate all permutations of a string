/* package whatever; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Ideone
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		String s = "abc";
		ArrayList<String> result = strPerm(s);
		System.out.println(result);
	}
	
	public static ArrayList<String> strPerm(String s)
	{
		ArrayList<String> perm = new ArrayList<String>();
		char[] schar = s.toCharArray();
		int j=0;
		perm.add(" ");
		while( j <= schar.length )
		{
			perm.addAll(genPerm(perm.get(0),schar[j]));
			//System.out.println(perm);
			if(perm.get(0) == " ")
				j = j+1;
			else if(perm.get(0).length() < perm.get(1).length())
				j = j+1;
			perm.remove(0);
			System.out.println(perm);
		}
	   
		return perm;
	}
	
	public static ArrayList<String> genPerm(String permutations, char addchar)
	{
		//System.out.println(permutations+"       "+addchar);
		ArrayList<String> p = new ArrayList<String>();
		if(permutations.equals(" "))
		{
			p.add(addchar+"");
		}
		else
		{
			for(int i =0; i<=permutations.length();i++)
			{
				String newp = permutations.substring(0,i).trim() + addchar + permutations.substring(i,permutations.length()).trim();
				//System.out.println(newp);
				p.add(newp);
			}
		}
			//System.out.println(p);
			return p;
	}
}
